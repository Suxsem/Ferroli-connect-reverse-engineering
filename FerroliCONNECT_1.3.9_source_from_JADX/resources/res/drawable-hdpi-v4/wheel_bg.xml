<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/cloudwarm_background" android:endColor="@color/cloudwarm_background" android:angle="90" android:centerColor="@color/cloudwarm_background"/>
            <stroke android:width="1dp" android:color="#f4f8e8"/>
        </shape>
    </item>
    <item android:left="4dp" android:top="1dp" android:right="4dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/cloudwarm_background" android:endColor="@color/cloudwarm_background" android:angle="90" android:centerColor="@color/cloudwarm_background"/>
        </shape>
    </item>
</layer-list>
